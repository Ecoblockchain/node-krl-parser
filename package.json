{
  "name": "krl-parser",
  "version": "0.0.0",
  "description": "Parse KRL source code into an AST",
  "main": "index.js",
  "scripts": {
    "start": "onchange '*.js' '*.ne' -e 'grammar.js' -i -- npm test -s",
    "build": "nearleyc krl.ne > grammar.js",
    "test": "npm run build -s && node tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farskipper/node-krl-parser.git"
  },
  "keywords": [
    "krl",
    "parser"
  ],
  "author": "farskipper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/farskipper/node-krl-parser/issues"
  },
  "homepage": "https://github.com/farskipper/node-krl-parser#readme",
  "devDependencies": {
    "lodash": "^4.13.1",
    "onchange": "^2.4.0",
    "tape": "^4.5.1"
  },
  "dependencies": {
    "nearley": "^2.5.0"
  }
}
